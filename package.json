{
  "name": "react-native-cloudinary-image-display",
  "version": "1.0.1",
  "description": "React Native Cloudinary Image Display",
  "main": "./dist/index.js",
  "watch": {
    "build": "{src,test}/*.js"
  },
  "scripts": {
    "watch": "npm-watch",
    "clean": "if [ -d ./dist ]; then rm -r ./dist; fi;mkdir ./dist;",
    "build-js": "babel -d dist/ src/",
