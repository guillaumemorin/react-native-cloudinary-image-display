    "lint": "eslint src/",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "author": "gmorin.pro@gmail.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumemorin/react-native-cloudinary-image-display.git"
  },
  "keywords": [
    "cloudinary",
    "image",
    "display"
  ],
  "bugs": {
    "url": "https://github.com/guillaumemorin/react-native-cloudinary-image-display/issues"
  },
  "homepage": "https://github.com/guillaumemorin/react-native-cloudinary-image-display#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "^22.4.3",
    "npm-watch": "^0.3.0"
  },
